<div id="user-login-form" class="row">
  <fieldset>
    <form class="offset2 span8">
      <h1>Login</h1>
      <hr />
      <label>Username</label>
      <input class="span4" type="text" id="login-field-username" placeholder="Username" required />
      <br />
      <label>Password</label>
      <input class="span4" type="password" id="login-field-password" placeholder="Password" required />
      <br />
      <button class="eduki-button-primary">Signup</button>
    </form>
  </fieldset>
</div>
