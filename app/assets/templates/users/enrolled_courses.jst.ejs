<div id="enrolled-courses">
  <h2>Enrolled Courses</h2>
  <% if (this.enrollments.size() <= 0) { %>
  <p>
    To enroll in a course, click the enrollment button <img src="<%- Eduki.Assets.imageUrl('icons/enroll.png') %>" alt="Enroll" /> on the course page.
    <a href="/#/courses" title="Browse Courses">Start browsing courses</a>!
  </p>
  <% } else { %>
    <div id="enrolled-courses-list">
      <div class="row">
      <% for (var i = 0; i < this.courses.size(); i++) { %>
          <div class="listing-square listing-enrolled-course">
            <div id="listing-<%- i % 6 %>" class="span2" title="<%- this.courses.models[i].get('title') %>"><span><%- this.courses.models[i].get('title') %></span></div>
            <div class="enrolled-course-overlay">
              <div class="enrolled-icons">
                <a class="eduki-button-icon course-home" href="/#/courses/<%- this.courses.models[i].get('id') %>"></a>
                <a class="eduki-button-icon quiz-attempts" href="/#/user/enrollment/<%- this.enrollments.models[i].get('id') %>/quizzes/"></a>
              </div>
            </div>
          </div>
        <% if ((i+1) % 6 == 0 && (i+1) < this.courses.size()) { %>
      </div>
      <div class="row">
        <% } %>
      <% } %>
    </div>
    <% } %>
</div>


